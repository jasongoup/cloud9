<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml" xmlns="http://www.w3.org/1999/xhtml">
 <a:tab name="buildTab">
    <a:style><![CDATA[
        .tab {
            padding : 0 0 0 0;
            border: 0px solid #c3c3c3;
            margin : 0 auto;
            overflow: hidden;
            position:relative;
            padding-top: 17px;
        }

        .tab .btncontainer{
            padding : 0 0 1px;
            z-index : 100;
            height: 25px;
            overflow: visible;
            white-space: nowrap;
            color: #404040;
            text-shadow : rgba(255, 255, 255, 1) 0px 1px 0px;
            font-family : Tahoma, Arial;
            font-size : 11px;
            border-bottom : 1px solid #8c8c8c;

            background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
            background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
            margin-top : -16px;
            width : auto;
        }

        .tab .btncontainer>div {
            display: block;
            display : inline-block;
            margin: 0 0 0 0;
            padding: 0;
            position : relative;
            vertical-align : bottom;
            cursor : default;
        }

        .tab .btncontainer .btntab {
            padding      : 5px 10px 2px 10px;
            position     : relative;
            margin-top:-1px;
            text-align: center;
            border-top : 1px solid #ffffff;
            border-right : 1px solid #8c8c8c;
            border-bottom : 1px solid #8c8c8c;
			height:25px;

            background: -webkit-gradient(linear,left bottom,left top,color-stop(0.5, rgb(199,199,199)),color-stop(1, rgb(214,214,214)));
            background: -moz-linear-gradient(center bottom,rgb(199,199,199) 50%,rgb(214,214,214) 100%);
        }

        /*.tab .btncontainer div.firstbtn {
            margin-left: 0;
        }*/

        .tab .btncontainer .btntab .tab_icon {
            height:16px;
            left:1px;
            position:absolute;
            top:1px;
            width:16px;
        }

        .tab .btncontainer .btntab .tab_title {
            overflow : hidden;
            padding  : 0 0 0 0;
        }

        .tab .btncontainer .btntab.btnclose .tab_title {
            margin-right : 15px;
        }

        .tab .scale .btntab .tab_title{
            display : block;
            overflow : hidden;
            text-overflow : ellipsis;
            white-space : nowrap;
        }

        .tab .btncontainer div.over {
            background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
            background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
            cursor: default;
        }

        .tab .btncontainer div.curbtn {
            color       : #202020;
            cursor      : default;
            z-index     : 1;
            background: -webkit-gradient(linear,left bottom,left top,color-stop(0.6 rgb(246,246,246)),color-stop(1, rgb(237,237,237)));
            background: -moz-linear-gradient(center bottom,rgb(246,246,246) 60%,rgb(237,237,237) 100%);
            border-right : 1px solid #8c8c8c;
            border-top: 1px solid #f9f9f9;
            border-bottom: 1px solid #F6F6F6;
        }

        .tab .scale .btntab .tab_middle {
            display : block;
            overflow : hidden;
            text-overflow : ellipsis;
            white-space : nowrap;
        }

        .tabFocus .curbtn{
        }

        .tab .btncontainer .btnclose strong {
            width               : 14px;
            height              : 14px;
            display             : block;
            position            : absolute;
            right               : 0;
            top                 : 7px;
            background-image    : url(images/close_tab_btn.png);
            background-repeat   : no-repeat;
            background-position : 0 -14px;
        }

        .tab .btncontainer .btnclose strong:hover {
            background-position : -14px -14px;
        }

        .tab .btncontainer .btnclose strong:active {
            background-position : -28px -14px;
        }

        .tab .btncontainer .curbtn.btnclose strong {
            background-position : 0 0;
            top: 7px;
        }

        .tab .btncontainer .curbtn.btnclose strong:hover {
            background-position : -14px 0;
        }

        .tab .btncontainer .curbtn.btnclose strong:active {
            background-position : -28px 0;
        }

        .tab .page {
            display : none;
            position : absolute;
            top : 25px;
            bottom : 0;
            left : 0;
            right : 0;
            z-index : 10;
            margin: 0 auto;
            background-color: #f6f6f6;
            overflow: hidden;
            clear: both;
            font-family : Arial;
            font-family: 12px;
        }

        .tab .page.curpage{
            display : block;
        }
    ]]></a:style>

    <a:presentation>
        <a:main pages="." buttons="div[1]">
            <div class="tab">
                <div class="btncontainer"> </div>
            </div>
        </a:main>
        <a:button caption="div[1]/text()" container="div" maxwidth="90" minwidth="10">
            <div class="btntab">
                <!--div class="tab_icon"> </div-->
                <div class="tab_title">-</div>
            </div>
        </a:button>
        <a:btnclose>
            <strong> </strong>
        </a:btnclose>
        <a:page container=".">
            <div class="page"> </div>
        </a:page>
    </a:presentation>
</a:tab>

<a:tree name="pjbuildtree">
	<a:style><![CDATA[
	.pjico{
	
	}
	.caption{
	color:ccc;
	}
	 ]]></a:style>

    <a:presentation>
        <a:main container="." startclosed="false">
            <div class="filemgr-tree">

            </div>
        </a:main>
        <a:item
          class     = ". pjico"
          caption   = "div/div/label/u"
          icon      = "div/div/label"
          openclose = "div/span"
          select    = "div/div/label"
          container = "following-sibling::u">
            <div class="item-fix">
                <div class="item">
                    <span>x</span>
                    <div class="label-content">
                        <label>
                            <u class="caption">-</u>
                        </label>
                    </div>
                </div>
            </div>
            <u> </u>
        </a:item>
        <a:dragindicator>
            <div class="filemgr-dragtree">
                <div class="item">
                    <span> </span>
                    <div class="label-content">
                        <label>
                            <u class="caption">-</u>
                        </label>
                    </div>
                </div>
            </div>
        </a:dragindicator>
        <a:loading>
            <div class="ldng">
                <span> </span>
                <label></label>
            </div>
        </a:loading>
        <a:empty caption=".">
            <div class="message"></div>
        </a:empty>
    </a:presentation>
</a:tree>

</a:skin>
