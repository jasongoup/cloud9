    <a:tab name="pkgattr_tab">
        <a:style><![CDATA[
            .pkgattr_tab {
                padding : 0 0 0 0;
                border: 0px solid #c3c3c3;
                margin : 0 auto;
                overflow: hidden;
                position:relative;
            }

            .pkgattr_tab .btncontainer{
                float: top;
                z-index : 100;
                white-space: nowrap;
                color: #404040;
                text-shadow : rgba(255, 255, 255, 1) 0px 1px 0px;
                font-family : Tahoma, Arial;
                font-size : 11px;

                background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
                background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
                width : auto;
            }

            .pkgattr_tab .btncontainer>div {
                display: block;
                display : inline-block;
                vertical-align : bottom;
                margin: 2 0 2 0;
                padding: 2;
                position : relative;
            }

            .pkgattr_tab .btncontainer .btntab {
                padding      : 2px;
                position     : relative;
                border-bottom : 1px solid #8c8c8c;
                cursor : default;
                width : 25px;
                background: -webkit-gradient(linear,left bottom,left top,color-stop(0.5, rgb(234,234,234)),color-stop(1, rgb(255,255,255)));
                background: -moz-linear-gradient(center bottom,rgb(234,234,234) 50%,rgb(255,255,255) 100%);
            }

            .pkgattr_tab .btncontainer .btntab .tab_icon {
                height:16px;
                left:1px;
                position:absolute;
                top:1px;
                width:16px;
            }

            .pkgattr_tab .btncontainer .btntab .tab_title {
                overflow : hidden;
                padding  : 2 0 2 0;
                font-size: 12px;
            }

            .pkgattr_tab .btncontainer .btntab.btnclose .tab_title {
                margin-right : 15px;
            }

            .pkgattr_tab .scale .btntab .tab_title{
                display : block;
                overflow : hidden;
                text-overflow : ellipsis;
                white-space : nowrap;
            }

            .pkgattr_tab .btncontainer div.over {
                background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
                background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
            }

            .pkgattr_tab .btncontainer div.curbtn {
                color       : #202020;
                z-index     : 1;
                background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(209,209,209)),color-stop(1, rgb(227,227,227)));
                background: -moz-linear-gradient(center bottom,rgb(209,209,209) 0%,rgb(227,227,227) 100%);
                -webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.2) inset;
                -moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.2) inset;
            }

            .pkgattr_tab .scale .btntab .tab_middle {
                display : block;
                overflow : hidden;
                text-overflow : ellipsis;
                white-space : nowrap;
            }

            .pkgattr_tab .page {
                display : none;
                position : absolute;
                padding: 0 2 0 2;
                top : 50;
                bottom : 5;
                left : 0;
                right : 0;
                z-index : 10;
                background-color: #f6f6f6;
                overflow: auto;
                clear: both;
                font-family : Arial;
                font-family: 12px;
            }

            .pkgattr_tab .page.curpage{
                display : block;
                border-left : 1px solid #8c8c8c;
            }
        ]]></a:style>

        <a:presentation>
            <a:main pages="." buttons="div[1]">
                <div class="pkgattr_tab">
                    <div class="btncontainer"/>
                </div>
            </a:main>
            <a:button caption="div[1]/text()" container="div" maxwidth="90" minwidth="10">
                <div class="btntab">
                    <!--div class="tab_icon"> </div-->
                    <div class="tab_title">-</div>
                </div>
            </a:button>
            <a:btnclose>
                <strong/>
            </a:btnclose>
            <a:page container=".">
                <div class="page"/>
            </a:page>
        </a:presentation>
    </a:tab>
    <a:list name="pkglist">
        <a:style><![CDATA[
            .listPS {
                overflow: hidden;
                position: relative;
                cursor: default;
                height: "250px";
            }

            .listPS .rcItem{
                color      : #cecece;
                text-shadow   : #000 0px 1px 0px;
                padding    : 0 2px 0 29px;
                position   : relative;
                height     : 20px;
                line-height: 19px;
                border-top:1px solid transparent;
                border-bottom:1px solid transparent;
            }

            .listPS .content{
                top: 1px;
                cursor : pointer;
                font-family : Arial, Tahoma;
                font-size : 11px;
                padding : 0 0 0 0;
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
                width: 115px;
                height: 20px;
            }

            .listPS .content div{
                font-family : Courier New;
                top: 1px;
                #padding : 2px 0 0 0;
                font-size : 12px;
                white-space : nowrap;
                overflow : hidden;
            }

            .listPS .radiobutton {
                width      : 16px;
                height     : 17px;
                background : url("images/radio-black.png") no-repeat 0 0;
                display : block;
                position : absolute;
                left : 2px;
                top : 2px;
            }

            .listPS .rcItem .radiobutton:hover{
                background-position : 0 -102px;
            }

            .listPS .rcItem .radiobutton:active{
                background-position : 0 -17px;
            }

            .listPS .active .radiobutton:active {
                background-position : 0 -51px !important;
            }

            .listPS .active .radiobutton {
                background-position : 0 -34px;
            }

            .listPS .active .radiobutton:hover{
                background-position : 0 -102px;
            }

            .listPS .selected {
                border-top:1px solid #55a2e0;
                border-bottom:1px solid #1166aa;
                filter     : progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#3692db', endColorstr='#1680d5');
                -ms-filter : "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#3692db', endColorstr='#1680d5')";
                background : -webkit-gradient(linear, left top, left bottom, from(#3692db), color-stop(1, #1680d5));
                background : -moz-linear-gradient(center bottom, #1680d5 0%, #3692db 100%) repeat scroll 0 0 transparent;
                color: #ffffff;
            }


            .listPSDisabled .radiobutton {
                background-position : 0 -68px;
            }

            .listPSDisabled .rcItem {
                color : #bebebe;
            }

            .listPS .selected .btnDelete {
                background-position : 0 0;
            }

            .listPS .rcItem:hover .btnDelete {
                width               : 16px;
                height              : 19px;
                position            : absolute;
                right               : 8px;
                top                 : 0px;
                background-image    : url(images/delete_btn2.png);
                background-repeat   : no-repeat;
                background-position : 0 0;
            }

            .listPS .selected .btnDelete:hover {
                background-position : -15px 0;
            }

            .listPS .selected .btnDelete:active {
                background-position : -31px 0;
            }
            .listPSDisabled .radiobutton {
                background-position : 0 -68px;
            }

            .listPSDisabled .rcItem {
                color : #bebebe;
            }

            .listPS .selected .btnDelete {
                background-position : 0 0;
            }

            .listPS .selected .btnDelete:hover {
                background-position : -15px 0;
            }

            .listPS .selected .btnDelete:active {
                background-position : -31px 0;
            }

            .listPS #txt_rename{
                border: 1px solid #bbb;
                padding: 0px 15px 1px 1px;
                color: black;
                text-shadow   : none;
                word-break: keep-all;
                overflow: visible;
                margin-top : 0px;
                cursor: text;
                height: 13px;
                width: 100px;
                display: block;
            }

            .listPS .selected .btnDelete1 {
                background-position : 0 0;
            }

            .listPS .selected .btnDelete1:hover {
                background-position : -15px 0;
            }

            .listPS .selected .btnDelete1:active {
                background-position : -31px 0;
            }
        ]]></a:style>

        <a:presentation>
            <a:main container=".">
                <div class="listPS">
                </div>
            </a:main>
            <a:item class="." caption="div" icon="div" select="." check="span">
                <div class="rcItem">
                    <span class="radiobutton"/>
