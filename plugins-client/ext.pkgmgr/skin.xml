    <a:window name="opkDialog">
        <a:style><![CDATA[
            div .dlgMain {
                height: 240px;
            }

            .col {
                float: left;
                height: 100%;
                position: relative;
            }

            .col1 {
                width: 175px;
                -webkit-transition: all .2s ease;
                -moz-transition: all .2s ease;
                transition: all .2s ease;
            }

            .col1:after {
                border-left: 1px solid rgba(0,0,0,.4);
                content: "";
                display: block;
                height: 100%;
                position: absolute;
                right: 0;
                top: 0;
                width: 0;
                /*width: 100px;*/
            }

            .col2 {
                width: 320px;
            }

            .col2:after {
                border-left: 1px solid rgba(255,255,255,.1);
                /*content: "";*/
                display: block;
                height: 100%;
                left: 0px;
                right: 0px;
                position: absolute;
                top: 0px;
                width: 10px
                ;
            }

            .label {
                line-height: 25px;
            }

            .list {
                height: 100%;
                overflow: auto;
                position: relative;
                border: 1px solid #C3C3C3;
                background-color: #FAFBFC;
                cursor: default;
            }

            div#actionBar {
                width: 100%;
                position: absolute;
                right: 0px;
                bottom: 0px;
            }

            .table table {
                width: 100%;
            }
        ]]></a:style>
    </a:window>

